-- ORDERS テーブル作成

COLUMN TABLE "ORDERS" (
  "ORDERID" INTEGER 
, "PRODUCTID" INTEGER 
, "CUSTOMERID" INTEGER 
, "EMPLOYEEID" INTEGER 
, "ORDERPERIODID" INTEGER 
, "REQUIREDPERIODID" INTEGER 
, "SHIPPEDPERIODID" INTEGER 
, "SHIPPERID" INTEGER 
, "UNITPRICE" INTEGER 
, "QUANTITY" INTEGER 
, "DISCOUNT" INTEGER 
, "UNITSALES" BIGINT 
, "NETSALES" BIGINT 
, "UNITCOST" INTEGER 
, "COST" INTEGER
,  PRIMARY KEY ("ORDERID", "PRODUCTID")
)

